Taylor
Teagan
Trinidad
Val
Winter
Zara
"""


def main():
    parser = argparse.ArgumentParser(description="Pick N names.")
    parser.add_argument('count', metavar='N', type=int, nargs='?',
                        help="number of names", default=5)
    args = parser.parse_args()
    names = namelist.strip().split("\n")
    selected = random.sample(names, args.count)
    for name in selected:
        print(name)


if __name__ == '__main__':
    main()
