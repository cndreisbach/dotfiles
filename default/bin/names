#!/usr/bin/env python

from __future__ import print_function
import random
import argparse

namelist = """
Addison
Adrian
Aidan
Alex
Alexis
Allison
Amari
Angel
Ari
Ariel
Autumn
Avery
Blake
Brody
Brook
Cadence
Carter
Casey
Chance
Charlie
Chase
Courtney
Dakota
Dale
Dallas
Dorian
Drew
Elliot
